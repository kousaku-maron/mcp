{
  "name": "@kousaku-maron/o3-search",
  "version": "0.0.4",
  "type": "module",
  "author": "@kousaku-maron",
  "license": "MIT",
  "description": "mcp server for web search by OpenAI o3 model",
  "bin": {
    "@kousaku-maron/o3-search": "./build/index.js"
  },
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "types": "./build/index.d.ts"
    }
  },
  "files": [
    "build",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kousaku-maron/mcp.git"
  },
  "bugs": {
    "url": "https://github.com/kousaku-maron/mcp/issues"
  },
  "homepage": "https://github.com/kousaku-maron/mcp#readme",
  "keywords": [
    "mcp",
    "claude",
    "openai",
    "o3",
    "chatgpt",
    "llm"
  ],
  "scripts": {
    "build": "tsc && chmod +x build/index.js",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.2",
    "openai": "^5.8.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^20.19.0",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@9.12.3"
}
